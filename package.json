{
  "name": "parvan-designsystem",
  "version": "0.1.31",
  "description": "Parvan Pajooh frontend components",
  "private": false,
  "keywords": [
    "react",
    "typescript",
    "storybook",
    "parvan",
    "parvan_components"
  ],
  "files": [
    "lib",
    "README.md"
  ],
  "main": "lib/index.cjs",
  "module": "lib/index.esm.js",
  "types": "lib/types/types.d.ts",
  "author": "pourya ataee",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pourya-ataee/parvan-component"
  },
  "bugs": {
    "url": "https://github.com/pourya-ataee/parvan-component/issues"
  },
  "homepage": "https://github.com/pourya-ataee/parvan-component#readme",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.23",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "clsx": "^1.2.1",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.9",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.9",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public",
    "win:build": "(if exist lib rmdir /s /q lib) && rollup -c && (if exist \"lib/stories\" rmdir /s /q \"lib/stories\") && (if exist \"lib/types/stories\" rmdir /s /q \"lib/types/stories\")",
    "build": "rm -rf lib && rollup -c && rm -rf \"lib/stories\" && rm -rf \"lib/types/stories\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.1.0",
    "@types/postcss-js": "^4.0.1",
    "node-sass": "^8.0.0",
    "postcss": "^8.4.23",
    "postcss-js": "^4.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.21.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-postcss-modules": "^2.1.1",
    "webpack": "^5.77.0"
  }
}
